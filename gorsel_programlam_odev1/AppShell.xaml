    <ShellContent
    Icon="kas.png"
     Title="vücut kitl indeksi"
     ContentTemplate="{DataTemplate local:Vucutkitle_Hesaplama}"
     Route="Vucutkitle_Hesaplama" />


    <ShellContent
   Icon="renkler.png"
     Title="Renk seçici"
   
     ContentTemplate="{DataTemplate local:Renk_secici}"
     Route="Renk_secici" />


    <ShellContent
   Icon="notepad.png"
     Title="Yapılacaklar"
     ContentTemplate="{DataTemplate local:yapilacaklar}"
     Route="yapilacaklar" />

</Shell>
