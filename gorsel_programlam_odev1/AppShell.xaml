    xmlns:local="clr-namespace:gorsel_programlam_odev1"
    Shell.FlyoutBehavior="Flyout"
    BackgroundColor="Yellow"
    >

    <ShellContent
       Icon="ev.png"
        
        Title="Ana sayıfa"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="Ana sayıfa" />

    <ShellContent
        
      Icon="hesap.png"
        Title="Kride Hsaplama"
        ContentTemplate="{DataTemplate local:kerdi_Hesaplama}"
